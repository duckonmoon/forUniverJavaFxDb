<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.update.UpdateStudentController">
   <children>
      <Label layoutX="263.0" layoutY="30.0" text="Update Student" />
      <TextField fx:id="nameTextField" layoutX="231.0" layoutY="150.0" promptText="name" />
      <TextField fx:id="surnameTextField" layoutX="231.0" layoutY="236.0" promptText="surname" />
      <TextField fx:id="identicalNumberTextField" layoutX="231.0" layoutY="317.0" promptText="identical number" />
      <Button fx:id="update" layoutX="278.0" layoutY="470.0" mnemonicParsing="false" text="Update" />
      <ComboBox fx:id="studentPicker" layoutX="82.0" layoutY="78.0" prefHeight="25.0" prefWidth="464.0" promptText="student" />
      <ComboBox layoutX="230.0" layoutY="386.0" prefWidth="150.0" fx:id="expelledPicker" promptText="expelled">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="YES" />
                  <String fx:value="NO" />
              </FXCollections>
          </items>
      </ComboBox>
   </children>
</AnchorPane>
